# Global shell definitions for the autotest test suite

PATH="/Users/imokhles/Downloads/dpkg/ios-build/dpkg-deb:/Users/imokhles/Downloads/dpkg/ios-build/dpkg-split:/Users/imokhles/Downloads/dpkg/ios-build/src:/Users/imokhles/Downloads/dpkg/ios-build/utils:$PATH"
export PATH

# Setup a sane environment

umask 0022

LC_ALL=C
export LC_ALL

TZ=UTC0
export TZ

SOURCE_DATE_EPOCH=0
export SOURCE_DATE_EPOCH

# Cleanup variables that might affect the tests.
unset GZIP
unset BZIP
unset XZ_OPT
unset XZ_DEFAULTS

if [ "$(id -u)" = 0 ]; then
  ASROOT=
elif which fakeroot >/dev/null; then
  ASROOT=fakeroot
else
  echo "skip: needs to run as root(ish)" >&2
  exit 0
fi
export ASROOT

TESTDATA="/Users/imokhles/Downloads/dpkg/ios-build/../t-func"
